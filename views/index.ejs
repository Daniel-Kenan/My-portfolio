            location.href = preview + "music" ;
           })
           
            $('.list').click(function(){
                const value = $(this).attr('data-filter');
                if(value == 'All'){
                    $('.col').show(animation_duration)
                }
                else{
                    
                    $('.col').not('.'+value).hide(animation_duration);
                    $('.col').filter('.'+value).show(animation_duration);
                }
            })

            $('.list').click(function(){
                // alert("hello")
                $(this).addClass('active').siblings().removeClass('active');
            })
        })
